{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES5",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src/",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["./declare.d.ts"]
}
